<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MathServiceService WSDL service -->

  <service name="MathService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.mindfire.com"/>
      <parameter name="wsdlServiceElement" value="MathServiceService"/>
      <parameter name="schemaQualified" value="http://service.mindfire.com"/>
      <parameter name="wsdlServicePort" value="MathService"/>
      <parameter name="className" value="com.mindfire.service.MathServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="MathService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="add" qname="operNS:add" xmlns:operNS="http://service.mindfire.com" returnQName="retNS:addReturn" xmlns:retNS="http://service.mindfire.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:i" xmlns:pns="http://service.mindfire.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:j" xmlns:pns="http://service.mindfire.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="substract" qname="operNS:substract" xmlns:operNS="http://service.mindfire.com" returnQName="retNS:substractReturn" xmlns:retNS="http://service.mindfire.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:i" xmlns:pns="http://service.mindfire.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:j" xmlns:pns="http://service.mindfire.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="substract add"/>

  </service>
</deployment>
